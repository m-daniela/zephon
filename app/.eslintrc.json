{
  "extends": ["next/babel","next/core-web-vitals"],
  "rules": {
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "indent": ["error", 4],
    "max-len": ["error", 100],
    "no-multi-spaces": ["error"],
    "no-multiple-empty-lines": ["error", { "max": 2, "maxEOF": 0 }],
    "no-unused-vars": "off",
    "react/prop-types": "off"
  },
    "ignorePatterns": [
    "node_modules",
    "build",
    "dist",
    "public"
  ]
}
